{
	"name": "directus-input-with-settings",
	"type": "module",
	"version": "0.1.0",
	"description": "Text input with a settings menu for configurable metadata like heading levels and sizes - stores as JSON",
	"repository": {
		"type": "git",
		"url": "https://github.com/bryantgillespie/directus-extensions.git",
		"directory": "packages/input-with-settings"
	},
	"icon": "extension",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-interface"
	],
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "interface",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "16.0.2",
		"typescript": "5.9.3",
		"vue": "3.5.22"
	}
}
